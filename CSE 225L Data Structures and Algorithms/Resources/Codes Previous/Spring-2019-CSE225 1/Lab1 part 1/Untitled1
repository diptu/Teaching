#include <iostream>

using namespace std;

int main()
{
    int a;
    cout << " enter number " << endl;
    cin>>a;
    int *p;
    p=new int [a];
    int i;
    for(i=0; i<a; i++);
    {
    cout << "element " << i+1<<" ";
    cin>>p[i];
    }
       for(i=0; i<a; i++);
    {
    cout << p[i] << " ";

    }
    delete []p;
    return 0;

}

